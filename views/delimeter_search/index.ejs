<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        td, th {
          border: 2px solid #dddddd;
          text-align: center;
          padding: 9px;
        }
        
        .mainbtns{
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
            align-items: center;
            width: 100%;
        }

        .SearchBar {    
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
            align-items: center;
            width: 100%;
                height: 100px;
                width: 50px;
                margin-left: 600px;
            }
        #search{
                  width: 300px;
                  height: 50px;
        }


        </style>
</head>
<body>
    <h2>Harsh Solanki</h2>

    <table>
        <tr>
            <th>FirstName</th>
            <th>LastName</th>
            <th>email</th>
            <th>Age</th>
            <th>MobNo</th>
            <th>City</th>
        </tr>
        <tr>
            <td>_ </td>
            <td>^</td> 
            <td>$</td>
            <td> } </td>
            <td> { </td>
            <td> :</td>
        </tr>
    </table>

    <div class="SearchBar">
        <form action="/delimeter" method="post">
            Search: <input type="search" placeholder="Search......." id="search" name="searchall" required tabindex="0" autofocus value=<%=search%>> 
             <input type="submit" value="Get" tabindex="1">
        </form>
    </div>
  
        </div> 
    <table>
        <tr>
            <th>Id</th>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Age</th>
            <th>MobNo</th>
            <th>email</th>
            <td>Gender</td>
            <td>DOB</td>
            <td>City</td>
            <td>IQ</td>
        </tr>
        <tr>
            <p><%=msg%></p>
          </tr>
        <% result.forEach(function(user) { %> 
            <tr>
                <td><%=user.Id%></td>
                <td><%=user.FirstName%></td>
                <td><%=user.LastName%></td>
                <td><%=user.Age%></td>
                <td><%=user.MobNo%></td>
                <td><%=user.email%></td>
                <td><%=user.Gender%></td>
                <td><%=user.DOB%></td>
                <td><%=user.IQ %></td>
                <td><%=user.City%></td>
            </tr>
          <% }); %>
         
    </table>
</body>
</html>